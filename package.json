{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "React / Webpack / Babel framework for quick set up",
  "main": "index.js",
  "scripts": {
    "open": "open http://localhost:6060",
    "clean": "rm -rf ./dist/*",
    "build": "webpack --config ./webpack.config.js --progress --colors",
    "build:prod": "NODE_ENV='production' webpack --config ./webpack.config.js --progress --colors",
    "start": "node ./app.js",
    "dev": "webpack-dev-server --inline --hot",
    "test": "karma start",
    "styleguide": "styleguidist server",
    "styleguide:build": "rm -rf ./styleguide/* && styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmcolella/webpack-boilerplate.git"
  },
  "author": "John Colella",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jmcolella/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/jmcolella/webpack-boilerplate#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "2.1.0",
    "lodash": "^4.17.4",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.3.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-autoreset": "^1.2.1",
    "postcss-discard-duplicates": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-initial": "^1.5.3",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-router": "^3.0.0",
    "react-styleguidist": "^5.0.6",
    "react-svg-inline": "^1.2.0",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.8",
    "webpack": "2.2.1",
    "webpack-combine-loaders": "^2.0.3",
