{
  "name": "athenaeum",
  "version": "0.0.4",
  "description": "PolicyGenius React Component Library",
  "main": "lib/index.js",
  "scripts": {
    "open": "open http://localhost:6060",
    "c": "yarn run plop create-component",
    "build": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.dev.js --progress --colors",
    "build:prod": "./node_modules/webpack/bin/webpack.js --config ./webpack.config.prod.js --progress --colors",
    "publish": "yarn run publish",
    "start": "yarn run styleguide:build && yarn run styleguide",
    "test": "karma start",
    "stylin": "yarn run start",
    "styleguide": "styleguidist server",
    "styleguide:build": "rm -rf ./styleguide/* && styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/policygenius/athenaeum.git"
  },
  "author": "John Colella",
  "license": "ISC",
  "homepage": "https://github.com/policygenius/athenaeum#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react-svg-inline": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.3.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.2",
